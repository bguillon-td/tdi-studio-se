<COMPONENT>
	<HEADER
		PLATEFORM="ALL"
		SERIAL=""
		VERSION="0.102"
		STATUS="ALPHA"

		COMPATIBILITY="ALL"
		AUTHOR="Talend"
		RELEASE_DATE="20050320A"

		DATA_AUTO_PROPAGATE="false"
		HAS_CONDITIONAL_OUTPUTS="true"
		SCHEMA_AUTO_PROPAGATE="true"
		VISIBLE="false"
		TECHNICAL="true"

		STARTABLE="false"
		PARTITIONING="AUTO"
	>
		<SIGNATURE />
	</HEADER>

	<FAMILIES>
		<FAMILY>Business/Marketo</FAMILY>
		<FAMILY>Cloud/Marketo</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
	    <CONNECTOR CTYPE="FLOW" MAX_OUTPUT="1" MAX_INPUT="1"/>
	    <!-- <CONNECTOR NAME="MAIN" CTYPE="FLOW" MAX_OUTPUT="1" BASE_SCHEMA="FLOW"/>-->
	    <CONNECTOR NAME="REJECT" CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1" LINE_STYLE="2" COLOR="FF0000" BASE_SCHEMA="FLOW"
	     NOT_SHOW_IF="(OPERATION=='syncMultipleLeads') or (DIE_ON_ERROR == 'true')"/>
	    <CONNECTOR CTYPE="ITERATE" MAX_INPUT="0" MAX_OUTPUT="0"/>
	    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
	    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
	    <CONNECTOR CTYPE="COMPONENT_OK"/>
	    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
	    <CONNECTOR CTYPE="RUN_IF"/>
	</CONNECTORS>

	<PARAMETERS>

		<PARAMETER
	      NAME="SCHEMA_FLOW"
	      FIELD="SCHEMA_TYPE"
	      NUM_ROW="1">
	      <TABLE>
	        <COLUMN NAME="Id" TYPE="id_Integer" READONLY="true" />
			<COLUMN NAME="Email" TYPE="id_String" />
			<COLUMN NAME="ForeignSysPersonId" TYPE="id_String" />
			<COLUMN NAME="ForeignSysType" TYPE="id_String" />
	      </TABLE>
	    </PARAMETER>

	    <PARAMETER
			NAME="SCHEMA_REJECT"
			FIELD="SCHEMA_TYPE"
			REQUIRED="true"
			NUM_ROW="1"
			CONTEXT="REJECT"
			SHOW="true"
		>
			<TABLE READONLY="true">
		        <COLUMN NAME="ERROR_MSG" TYPE="id_String" READONLY="false" CUSTOM="true"/>
		        <COLUMN NAME="Id" TYPE="id_Integer" READONLY="true" />
				<COLUMN NAME="Email" TYPE="id_String" />
				<COLUMN NAME="ForeignSysPersonId" TYPE="id_String" />
				<COLUMN NAME="ForeignSysType" TYPE="id_String" />
			</TABLE>
	    </PARAMETER>

		<PARAMETER
			NAME="ENDPOINT"
			FIELD="TEXT"
			NUM_ROW="10"
			REQUIRED="true"
		>
			<DEFAULT>"https://123-ABC-456.mktoapi.com/soap/mktows/2_7"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER
			NAME="SECRET_KEY"
			FIELD="PASSWORD"
			NUM_ROW="20"
			REQUIRED="true"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
		<PARAMETER
			NAME="CLIENT_ACCESSID"
			FIELD="TEXT"
			NUM_ROW="20"
			REQUIRED="true"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
		<PARAMETER
			NAME="OPERATION"
			FIELD="CLOSED_LIST"
			NUM_ROW="30"
			REQUIRED="true"
		>
			<ITEMS DEFAULT="syncLead">
				<ITEM NAME="syncLead" VALUE="syncLead" />
				<ITEM NAME="syncMultipleLeads" VALUE="syncMultipleLeads" />
			</ITEMS>
		</PARAMETER>
		
		<PARAMETER
			NAME="MAPPING_INPUT"
			FIELD="TABLE"
			NUM_ROW="40"
			NB_LINES="6"
			REQUIRED="true"
		>
			<ITEMS BASED_ON_SCHEMA="true">
				<ITEM NAME="PARAMETER_NAME" FIELD="String" VALUE='""'/>
			</ITEMS>
		</PARAMETER>
	
		<PARAMETER
			NAME="DE_DUPENABLED"
			FIELD="CHECK"
			NUM_ROW="50"
			REQUIRED="true"
			SHOW_IF="OPERATION=='syncMultipleLeads'"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER
			NAME="BATCH_SIZE"
			FIELD="TEXT"
			NUM_ROW="60"
			REQUIRED="true"
			SHOW_IF="OPERATION=='syncMultipleLeads'"
		>
			<DEFAULT>100</DEFAULT>
		</PARAMETER>
		
		<PARAMETER
			NAME="TIMEOUT"
			FIELD="TEXT"
			NUM_ROW="70"
			REQUIRED="true"
		>
			<DEFAULT>600000</DEFAULT>
		</PARAMETER>
	
		<PARAMETER
			NAME="DIE_ON_ERROR"
			FIELD="CHECK"
			NUM_ROW="100"
			REQUIRED="true"
		>
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
	</PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
			<IMPORT
				NAME="SOAP-axis"
				MODULE="axis.jar" MVN="mvn:org.talend.libraries/axis/6.0.0" 
				UrlPath="platform:/plugin/org.talend.libraries.apache.axis2/lib/axis.jar"
				REQUIRED="true" />
			<IMPORT
				NAME="SOAP-discovery"
				MODULE="commons-discovery-0.2.jar" MVN="mvn:org.talend.libraries/commons-discovery-0.2/6.0.0" 
				UrlPath="platform:/plugin/org.talend.libraries.apache.common/lib/commons-discovery-0.2.jar"
				REQUIRED="true" />
			<IMPORT
				NAME="SOAP-jaxrpc"
				MODULE="jaxrpc.jar" MVN="mvn:org.talend.libraries/jaxrpc/6.0.0" 
				UrlPath="platform:/plugin/org.talend.libraries.jax/lib/jaxrpc.jar"
				REQUIRED="true" />
			<IMPORT
				NAME="SOAP-saaj"
				MODULE="saaj.jar" MVN="mvn:org.talend.libraries/saaj/6.0.0" 
				UrlPath="platform:/plugin/org.talend.libraries.saaj/lib/saaj.jar"
				REQUIRED="true" />
			<IMPORT
				NAME="WSDL4J-1_6_3"
				MODULE="wsdl4j-1.6.3.jar" MVN="mvn:org.talend.libraries/wsdl4j-1.6.3/6.0.0" 
				BundleID="org.apache.servicemix.bundles.wsdl4j"
				REQUIRED="true" />
			<IMPORT
				NAME="SOAP-logging"
				MODULE="commons-logging-1.1.jar" MVN="mvn:org.talend.libraries/commons-logging-1.1/6.0.0" 
				UrlPath="platform:/plugin/org.talend.libraries.apache.common/lib/commons-logging-1.1.jar"
				REQUIRED="true" />
			<IMPORT
				NAME="SOAP-JavaMail"
				MODULE="mail.jar" MVN="mvn:org.talend.libraries/mail/6.0.0" 
				UrlPath="platform:/plugin/org.talend.libraries.apache.axis2/lib/mail.jar"
				REQUIRED="true" />
			<IMPORT
				NAME="SOAP-JAF"
				MODULE="activation.jar" MVN="mvn:org.talend.libraries/activation/6.0.0" 
				UrlPath="platform:/plugin/org.talend.libraries.apache.axis2/lib/activation.jar"
				REQUIRED="true" />
			<IMPORT
				NAME="XML-DOM4J"
				MODULE="dom4j-1.6.1.jar" MVN="mvn:org.talend.libraries/dom4j-1.6.1/6.0.0" 
				UrlPath="platform:/plugin/org.talend.libraries.dom4j-jaxen/lib/dom4j-1.6.1.jar"
				REQUIRED="true"
				BundleID="" />
			<IMPORT
				NAME="XML-XPATH-JAXEN"
				MODULE="jaxen-1.1.1.jar" MVN="mvn:org.talend.libraries/jaxen-1.1.1/6.0.0" 
				UrlPath="platform:/plugin/org.talend.libraries.dom4j-jaxen/lib/jaxen-1.1.1.jar"
				REQUIRED="true"
				BundleID="" />
			<IMPORT
				NAME="TALEND-MARKETO"
				MODULE="talend-marketo-2.7.jar" MVN="mvn:org.talend.libraries/talend-marketo-2.7/6.0.0" 
				REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>

	<RETURNS>
		<RETURN NAME="NB_CALL" TYPE="id_Integer" AVAILABILITY="AFTER" />
  	</RETURNS>

</COMPONENT>
