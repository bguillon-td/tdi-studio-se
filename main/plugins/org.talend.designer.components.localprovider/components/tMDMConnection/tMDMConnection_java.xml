<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"

    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
	<FAMILY>Talend MDM</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>

	<PARAMETER NAME="SERVERVERSION" FIELD="CLOSED_LIST" NUM_ROW="12" REQUIRED="true" REPOSITORY_VALUE="MDM_VERSION">
		<ITEMS DEFAULT="SERVER_6_0">
			<ITEM NAME="SERVER_6_0" VALUE="s60" />
			<ITEM NAME="SERVER_5_6" VALUE="s56" />
		</ITEMS>
	</PARAMETER>

    <PARAMETER NAME="MDMURL" FIELD="TEXT" NUM_ROW="20"
		REQUIRED="true">
		<DEFAULT>"http://localhost:8180/talendmdm/services/soap"</DEFAULT>
	</PARAMETER>

	<PARAMETER NAME="USERNAME" FIELD="TEXT" REQUIRED="true"
		NUM_ROW="30">
		<DEFAULT>"userName"</DEFAULT>
	</PARAMETER>

	<PARAMETER NAME="PASSWORD" FIELD="PASSWORD" REQUIRED="true"
		NUM_ROW="30">
		<DEFAULT>"password"</DEFAULT>
	</PARAMETER>

	<PARAMETER NAME="UNIVERSE" FIELD="TEXT"
		NUM_ROW="30" SHOW_IF="SERVERVERSION == 's56'">
		<DEFAULT></DEFAULT>
	</PARAMETER>
	
    <PARAMETER
	    NAME="AUTO_COMMIT"
	    FIELD="CHECK"
	    NUM_ROW="40">
	    <DEFAULT>true</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
	    NAME="GEN_TRANSACTION_ID"
	    FIELD="CHECK"
	    NUM_ROW="40"
	    SHOW_IF="AUTO_COMMIT=='false'"
	>
	    <DEFAULT>false</DEFAULT>
    </PARAMETER>

  </PARAMETERS>

  <ADVANCED_PARAMETERS/>

